    "require-dev": {
        "behat/mink": "~1.6",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.1",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.7"
    },
    "scripts": {
        "post-autoload-dump": "DruStack\\Standard\\Composer\\ScriptHandler::ensureHtaccess",
        "post-install-cmd": "DruStack\\Standard\\Composer\\ScriptHandler::installRequirementsFile",
        "post-package-install": [
            "DruStack\\Standard\\Composer\\ScriptHandler::vendorTestCodeCleanup",
            "DruStack\\Standard\\Composer\\ScriptHandler::generateInfoMetadata"
        ],
        "post-package-update": [
            "DruStack\\Standard\\Composer\\ScriptHandler::vendorTestCodeCleanup",
            "DruStack\\Standard\\Composer\\ScriptHandler::generateInfoMetadata"
        ],
        "post-update-cmd": "DruStack\\Standard\\Composer\\ScriptHandler::installRequirementsFile",
        "pre-autoload-dump": "DruStack\\Standard\\Composer\\ScriptHandler::preAutoloadDump"
    },
    "type": "project"
}
