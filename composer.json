{
    "authors": [
        {
            "email": "hswong3i@pantarei-design.com",
            "name": "Wong Hoi Sing Edison"
        }
    ],
    "autoload": {
        "psr-4": {
            "DruStack\\Standard\\": "src/"
        }
    },
    "description": "The \"DruStack Standard Edition\" distribution",
    "extra": {
        "branch-alias": {
            "dev-develop": "8.x-dev",
            "dev-master": "8.1.x-dev"
        },
        "installer-paths": {
            "web": [
                "type:drupal-core"
            ],
            "web/sites/all/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/sites/all/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
        }
    },
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "name": "drustack/framework-standard-edition",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "cweagans/composer-patches": "~1.0",
        "drupal/drupal": "~7.43",
        "drush/drush": "~8.0"
    },
    "require-dev": {
        "behat/mink": "~1.6",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.1",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.7"
    },
    "scripts": {
        "post-install-cmd": "DruStack\\Standard\\Composer\\ScriptHandler::installRequirementsFile",
        "post-update-cmd": "DruStack\\Standard\\Composer\\ScriptHandler::installRequirementsFile"
    },
    "type": "project"
}
