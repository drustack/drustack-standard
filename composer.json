{
    "authors": [
        {
            "email": "hswong3i@pantarei-design.com",
            "name": "Wong Hoi Sing Edison"
        }
    ],
    "autoload": {
        "psr-4": {
            "DruStack\\Standard\\": "src/",
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer/"
        }
    },
    "description": "The \"DruStack Standard Edition\" distribution",
    "extra": {
        "branch-alias": {
            "dev-develop": "0.x-dev",
            "dev-master": "0.0.x-dev"
        },
        "installer-paths": {
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        }
    },
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "name": "drustack/framework-standard-edition",
    "prefer-stable": true,
    "require": {
        "composer/installers": "~1.0",
        "cweagans/composer-patches": "~1.0",
        "drupal/console": "~0.10",
        "drupal/core": "~8.0",
        "drush/drush": "~8.0"
    },
    "require-dev": {
        "behat/mink": "~1.6",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.1",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.7"
    },
    "scripts": {
        "post-package-install": "DruStack\\Standard\\Composer\\ScriptHandler::installRequirementsFile",
        "post-package-update": "DruStack\\Standard\\Composer\\ScriptHandler::installRequirementsFile"
    },
    "type": "project"
}
