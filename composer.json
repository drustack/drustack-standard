{
    "authors": [
        {
            "email": "hswong3i@pantarei-design.com",
            "name": "Wong Hoi Sing Edison"
        }
    ],
    "autoload": {
        "classmap": [
            "src/Composer/ScriptHandler.php"
        ]
    },
    "config": {
        "discard-changes": true,
        "secure-http": false
    },
    "description": "The \"DruStack Standard Edition\" distribution",
    "extra": {
        "branch-alias": {
            "dev-develop": "8.x-dev",
            "dev-master": "8.1.x-dev"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "https://www.drupal.org/node/2619250": "https://www.drupal.org/files/issues/drupal-do_not_disable_MultiViews_htaccess-2619250-17.patch",
                "https://www.drupal.org/node/2682373": "https://www.drupal.org/files/issues/event-dispatcher-2682373-16.patch"
            },
            "drupal/google_analytics": {
                "https://www.drupal.org/node/2712793": "https://drupal.org/files/issues/google_analytics-remove_version_from_info_yml-2712793-1.patch"
            },
            "drupal/recaptcha": {
                "https://www.drupal.org/node/2712795": "https://drupal.org/files/issues/recaptcha-remove_version_from_info_yml-2712795-2.patch"
            },
            "drupal/sitemap": {
                "https://www.drupal.org/node/2712797": "https://drupal.org/files/issues/sitemap-remove_version_from_info_yml-2712797-2.patch"
            }
        }
    },
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "name": "drustack/framework-standard-edition",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "cweagans/composer-patches": "~1.0",
        "drupal/acl": "8.1.x-dev",
