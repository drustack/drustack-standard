{
    "authors": [
        {
            "email": "hswong3i@pantarei-design.com",
            "name": "Wong Hoi Sing Edison"
        }
    ],
    "autoload": {
        "psr-4": {
            "Drustack\\Standard\\": "src"
        }
    },
    "config": {
        "discard-changes": true,
        "process-timeout": 3600,
        "secure-http": false,
        "vendor-dir": "vendor"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "The \"DruStack Standard Edition\" distribution",
    "extra": {
        "branch-alias": {
            "dev-develop": "9.x-dev",
            "dev-master": "9.0.x-dev"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ]
    },
    "homepage": "https://drupal.org/project/drustack",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "name": "drustack/framework-standard-edition",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "composer/installers": "~1.9",
        "cweagans/composer-patches": "~1.6",
        "drupal/core": "~9.0.1",
        "drupal/core-composer-scaffold": "~9.0",
        "drupal/core-project-message": "~9.0",
        "drupal/core-recommended": "~9.0",
        "drupal/core-vendor-hardening": "~9.0"
    },
    "require-dev": {
        "drupal/core-dev": "~9.0",
        "drush/drush": "~10.3",
        "oomphinc/composer-installers-extender": "~1.1",
        "zaporylie/composer-drupal-optimizations": "~1.1"
    },
    "type": "project"
}
