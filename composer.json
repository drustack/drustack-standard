{
    "authors": [
        {
            "email": "hswong3i@pantarei-design.com",
            "name": "Wong Hoi Sing Edison"
        }
    ],
    "autoload": {
        "classmap": [
            "src/Composer/ScriptHandler.php"
        ]
    },
    "config": {
        "discard-changes": true,
        "secure-http": false
    },
    "description": "The \"DruStack Standard Edition\" distribution",
    "extra": {
        "branch-alias": {
            "dev-develop": "8.x-dev",
            "dev-master": "8.2.x-dev"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/bootstrap": {
                "https://drupal.org/node/2765505": "https://drupal.org/files/issues/bootstrap-anonymous_search_form_submit_fail-2765505-3.patch"
            },
            "drupal/core": {
                "https://drupal.org/node/2619250": "https://drupal.org/files/issues/drupal-do_not_disable_MultiViews_htaccess-2619250-24.patch",
                "https://drupal.org/node/2716019": "https://www.drupal.org/files/issues/core_views-implements_title_callback-2716019-23-D8.patch"
            },
            "drupal/eform": {
                "https://drupal.org/node/2749623": "https://drupal.org/files/issues/eform-remove_version_from_info_yml-2749623-2.patch"
            },
            "drupal/special_menu_items": {
                "https://drupal.org/node/2749911": "https://drupal.org/files/issues/special_menu_items-remove_version_from_info_yml-2749911-2.patch"
            },
            "drupal/token": {
                "https://drupal.org/node/2816693": "https://drupal.org/files/issues/token-term_vocabulary_source-2816693-2.patch"
            },
            "drupal/views_bootstrap": {
                "https://drupal.org/node/2462305": "https://drupal.org/files/issues/views_bootstrap-port_grid_plugin_to_d8-2462305-25.patch",
                "https://drupal.org/node/2730725": "https://drupal.org/files/issues/port_list_group_plugin_to-D8.patch"
            },
            "drupal/views_templates": {
                "https://drupal.org/node/2749745": "https://drupal.org/files/issues/views_templates-remove_version_from_info_yml-2749745-2.patch"
            },
            "drupal/xmlrpc": {
                "https://drupal.org/node/2749767": "https://drupal.org/files/issues/xmlrpc-remove_version_from_info_yml-2749767-2.patch"
            }
        }
    },
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "name": "drustack/framework-standard-edition",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "https://github.com/backupmigrate/backup_migrate_core"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/twbs/bootstrap/releases/download/v3.3.7/bootstrap-3.3.7-dist.zip"
                },
                "name": "twbs/bootstrap",
                "require": {
                    "composer/installers": "~1.0"
                },
                "type": "drupal-library",
                "version": "3.3.7"
            },
            "type": "package"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "http://download.ckeditor.com/codesnippet/releases/codesnippet_4.5.11.zip"
                },
                "name": "ckeditor/codesnippet",
