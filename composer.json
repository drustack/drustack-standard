    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "The \"DruStack Standard Edition\" distribution",
    "extra": {
        "branch-alias": {
            "dev-develop": "8.x-dev",
            "dev-master": "8.7.x-dev"
        },
        "installer-paths": {
            "web/core/": [
                "type:drupal-core"
            ],
            "web/drush/{$name}/": [
                "type:drupal-drush"
            ],
            "web/libraries/{$name}/": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}/": [
                "type:drupal-module"
            ],
            "web/modules/custom/{$name}/": [
                "type:drupal-custom-module"
            ],
            "web/profiles/{$name}/": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}/": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}/": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "https://drupal.org/node/2619250": "https://drupal.org/files/issues/drupal-do_not_disable_MultiViews_htaccess-2619250-24.patch",
                "https://drupal.org/node/2664016": "https://drupal.org/files/issues/2018-06-06/2664016-58.patch",
                "https://drupal.org/node/2673980": "https://drupal.org/files/issues/2019-04-05/rss_view_with_fields-2673980-99.patch",
