    "scripts": {
        "post-autoload-dump": [
            "DruStack\\Standard\\Composer\\ScriptHandler::ensureHtaccess"
        ],
        "post-install-cmd": [
            "DruStack\\Standard\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-package-install": [
            "DruStack\\Standard\\Composer\\ScriptHandler::vendorTestCodeCleanup",
            "DruStack\\Standard\\Composer\\ScriptHandler::generateInfoMetadata"
        ],
        "post-package-update": [
            "DruStack\\Standard\\Composer\\ScriptHandler::vendorTestCodeCleanup",
            "DruStack\\Standard\\Composer\\ScriptHandler::generateInfoMetadata"
        ],
        "post-update-cmd": [
            "DruStack\\Standard\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "pre-autoload-dump": [
            "DruStack\\Standard\\Composer\\ScriptHandler::preAutoloadDump"
        ],
        "pre-install-cmd": [
            "DruStack\\Standard\\Composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DruStack\\Standard\\Composer\\ScriptHandler::checkComposerVersion"
        ]
    },
